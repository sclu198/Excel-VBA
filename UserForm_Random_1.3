/* Code Block 1*/

Private Sub Workbook_Open()
    
    Randomize

    Characters = "0123456789ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz"
    letter_number = ""

    For j = 2 To 32
    For i = 1 To 14
        number_length = Int(Len(Characters) * Rnd) + 1
        letter_number = letter_number & Mid(Characters, number_length, 1)
        If i = 2 Then letter_number = letter_number & "-"
        If i = 10 Then letter_number = letter_number & "-"

    Next
    Range("C" & j) = letter_number      'enters the text follow by letter_number in each cell from C2 to C32
    letter_number = ""
    Next
    
End Sub
